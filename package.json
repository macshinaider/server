{
  "name": "server",
  "version": "4.15.18",
  "description": "Server components for albion-killbot.",
  "repository": "https://github.com/black-river-gaming/albion-killbot.git",
  "author": "Agnaldo Junior <agnaldo.junior01@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node ./bin/start.js",
    "start:api": "npm start -- --mode api",
    "start:api:dev": "nodemon --config src/interfaces/api/nodemon.json",
    "start:bot": "npm start -- --mode bot",
    "start:bot:dev": "nodemon --config src/interfaces/bot/nodemon.json",
    "start:crawler": "npm start -- --mode crawler",
    "start:crawler:dev": "nodemon --config src/interfaces/crawler/nodemon.json",
    "db:migrate": "node ./bin/migrate",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier --write ."
  },
  "engines": {
    "node": "16.x",
    "npm": "^8.x"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.319.0",
    "amqplib": "^0.9.1",
    "axios": "^0.27.2",
    "canvas": "^2.6.1",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "datadog-winston": "^1.5.1",
    "discord.js": "^14.10.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.5.1",
    "express-session": "^1.17.3",
    "fast-redact": "^3.1.2",
    "i18n": "^0.14.2",
    "minimist": "^1.2.6",
    "moment": "^2.29.4",
    "mongodb": "^4.6.0",
    "morgan": "^1.10.0",
    "sharp": "^0.30.7",
    "stripe": "^9.8.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "winston": "^3.8.1",
    "winston-loggly-bulk": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.6.2"
  }
}
